Config 
  { font =    "xft:Bitstream Vera Sans Mono:size=12:antialias=true"
  -- #FFBD4A - nice
  -- #7092FF
  -- #FF3A33 
  , allDesktops = True
  , persistent  = False
  , template = "| <fc=#7092FF>%XMonadLog%</fc> | %battery% | %bright% | %coretemp% | %alsa:default:Master% | %wlp2s0wi%%dynnetwork% }{| %date% | %kbd% |"
  , commands = 
  [ Run Kbd [("us", "US"), ("ru", "RU")]
  , Run Date "<fc=#7092FF>%F</fc> (%a) <fc=#FFBD4A>%T</fc>" "date" 10
  , Run Battery 
    [ "--template", "Battery: <acstatus>"
    , "--Low", "35", "--High", "60"
    , "--low", "#FF3A33", "--high", "#7092FF", "--normal", "#FFBD4A"
    , "--" -- battery specific options
      , "-o", "<left>"
      , "-O", "<left>â€¦"
      , "-i", "80"
    ] 10
  , Run CoreTemp 
    [ "--Low", "45", "--High", "70"
    , "--low", "#7092FF", "--normal", "#FFBD4A", "--high", "#FF3A33"
    , "--template", "Temp: <core0>"
    ] 10
  , Run DynNetwork 
    [ "--template", ": <tx>/<rx> |"
    , "--normal", "#7092FF"
    , "--high", "#FFBD4A"
    , "--Low", "-1", "--High", "10000"
    ] 40
  , Run Wireless "wlp2s0" 
    [ "--template", "<essid>(<quality>)"
    , "--normal", "#7092FF", "--low", "#7092FF"
    , "--Low", "40", "--High", "110"
    ] 40
  , Run Brightness
    [ "--template", "Brightness: <percent>"
    , "--Low", "65", "--High", "101"
    , "--low", "#7092FF"
    , "--normal", "#FFBD4A"
    , "--" 
    , "-D", "intel_backlight"
    ] 10
  , Run Alsa "default" "Master"
    [ "--normal", "#FFBD4A"
    , "--low", "#7092FF"
    , "--high", "#FF3A33"
    , "--Low", "70", "--High", "750"
    , "--template", "Volume: <volume> <status>"
    , "--"
      , "--onc", "#7092FF"
      , "--offc", "#FFBD4A"
    ]
  , Run Com "echo" ["I'll wait for as long as it takes!"] "" 0
  , Run XMonadLog
  ]

  }
